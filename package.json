{
  "name": "cypress-test",
  "version": "1.0.0",
  "scripts": {
    "open": "npx cypress open --browser chrome --env configFile=qat",
    "test": "npx cypress run --browser chrome --env configFile=qat --headed",
    "allure:report": "npx allure generate reports/allure-results --clean -o reports/allure-report",
    "allure:open": "npx allure open reports/allure-report",
    "clean-report": "rimraf reports",
    "clean-run": "npm run clean-report && npm run test",
    "clean-run-report": "(npm run clean-run || true) && npm run allure:report && npm run allure:open",
    "test:ci": "cypress run && npm run allure:report",
    "testp": "npx cypress run",
    "parallel": "node_modules/.bin/cypress-parallel -s cy:testp -t 3 -m false -d 'cypress/e2e/**/*.feature' --weightsJson 'cypress/parallel-weights.json' -n 'node_modules/cypress-multi-reporters' -a '\"--browser chrome --env configFile=qat\"'",
    "tags": "set DEBUG=true && cypress-tags run --glob \"cypress/e2e/agent-login/*.feature\" -e TAGS=\"@test\" --browser chrome --env configFile=qat --config numTestsKeptInMemory=0",
    "fm": "npx prettier . --write",
    "lint": "npx eslint ."
  },
  "private": true,
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "20.0.0",
    "@cypress/browserify-preprocessor": "3.0.2",
    "@cypress/skip-test": "2.6.1",
    "@faker-js/faker": "7.5.0",
    "@mmisty/cypress-allure-adapter": "0.16.2",
    "@types/chai-subset": "1.3.3",
    "@types/fs-extra": "11.0.4",
    "@types/jsonpath": "0.2.4",
    "@types/lodash": "4.14.202",
    "@types/luxon": "2.3.2",
    "@types/node": "20.11.7",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "allure-commandline": "2.17.2",
    "axios": "1.6.7",
    "chai": "4.3.6",
    "chai-json-schema": "1.5.1",
    "chai-subset": "1.6.0",
    "codelyzer": "0.0.28",
    "cypress": "13.5.0",
    "cypress-failed-log": "2.10.0",
    "cypress-log-to-output": "1.1.2",
    "cypress-multi-reporters": "1.6.0",
    "cypress-network-idle": "1.14.2",
    "cypress-parallel": "https://gitpkg.now.sh/thuongdv/cypress-parallel/lib",
    "cypress-timings": "1.0.0",
    "cypress-xpath": "1.6.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "glob": "7.1.6",
    "gmail-tester": "github:thuongdv/gmail-tester",
    "json2csv": "5.0.7",
    "jsonpath": "1.1.1",
    "jwt-decode": "3.1.2",
    "kill-process-by-name": "1.0.5",
    "lodash": "4.17.21",
    "luxon": "2.5.2",
    "node-html-parser": "5.3.3",
    "resolve": "1.22.2",
    "rimraf": "3.0.2",
    "statman-stopwatch": "2.18.0",
    "ts-node": "8.3.0",
    "typescript": "5.3.3",
    "uuid": "9.0.0",
    "xlsx": "0.17.5"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.steps.ts",
      "cypress/e2e/[filepath].steps.ts",
      "cypress/support/step-definitions/**/*.steps.ts"
    ]
  }
}
